FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (10 0 0);

boundaryField
{
	FarField_extruded
	{
		type            freestream;
        	freestreamValue uniform (10 0 0);	
	}
	Airfoil_extruded
	{
		type		noSlip;
	}

	TopLeft
	{
        	type            empty;
	}
	BottomLeft
	{
       		type            empty;
	}
	TopRight
	{
		type            empty;
	}
	BottomRight
	{
       		type            empty;
	}
	TopLeft_top
	{
	       	type            empty;
	}
	BottomLeft_top
	{
	       	type            empty;
	}
	TopRight_top
	{
	       	type            empty;
	}
	BottomRight_top
	{
	       	type            empty;
	}
}

